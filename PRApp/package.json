{
  "name": "carmerceUser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.dev source ~/.bash_profile&&react-native run-android",
    "android-dev": "ENVFILE=.env.dev source ~/.bash_profile&&react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android-stg": "ENVFILE=.env.stage source ~/.bash_profile&&react-native run-android --variant=stageDebug --appIdSuffix=stg",
    "android-prd": "ENVFILE=.env source ~/.bash_profile&&react-native run-android --variant=productionDebug",
    "android-dev-apk": "cd android && ./gradlew clean && ENVFILE=.env.dev ./gradlew assembleDevRelease && cd ../",
    "android-clean": "cd android && ./gradlew clean && cd ../",
    "ios": "react-native run-ios",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios --configuration Dev --scheme carmerceUser.dev",
    "ios-stg": "ENVFILE=.env.stage react-native run-ios --configuration Stage --scheme carmerceUser.stage",
    "ios-prd": "ENVFILE=.env react-native run-ios --scheme carmerceUser",
    "ios-install": "cd ios && pod install && cd ..",
    "ios-i": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
